[
  {
    "name": "Work-Documents-Current",
    "match": {
      "extensions": ["docx", "xlsx", "pptx", "pdf"],
      "pathRegex": "(?i)(work|project|client|business)",
      "ageDaysMax": 90
    },
    "target": {
      "location": "OneDrive://02_Work/Current/",
      "namingTemplate": "StandardDateContextTitle"
    },
    "conflictPolicy": "VersionSuffix",
    "priority": 350
  },
  {
    "name": "Work-Documents-Archive",
    "match": {
      "extensions": ["docx", "xlsx", "pptx", "pdf"],
      "pathRegex": "(?i)(work|project|client|business)",
      "ageDaysMin": 90
    },
    "target": {
      "location": "OneDrive://02_Work/Archive/",
      "namingTemplate": "StandardDateContextTitle"
    },
    "conflictPolicy": "TimestampSuffix",
    "priority": 320
  },
  {
    "name": "Meeting-Notes",
